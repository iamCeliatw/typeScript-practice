{
  "compilerOptions": {
    "noImplicitAny": true,
    "target": "es5",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "*": ["node_modules/@types/*", "src/types/*"]
    },
    "moduleResolution": "node"
  },
  "include": ["src"]
}
